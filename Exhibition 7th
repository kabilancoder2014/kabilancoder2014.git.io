/* Pull the hero up so the carousel sits flush under the fixed navbar. 
   We use negative margin equal to navbar height and add matching top padding
   so content (if any) inside the hero isn't hidden behind the bar. */
.hero {
  /* Pull hero up underneath the fixed navbar but do not add extra offset padding */
  margin-top: calc(-1 * var(--navbar-h));
  /* only reserve the navbar height so content aligns flush under it */
  padding-top: var(--navbar-h);
}

