/* Hero illustration animations */
.hero-illustration { max-height: 300px; width: 100%; display: block; }
.hero-illustration .bar { transform-origin: bottom; transform: scaleY(0); opacity: 0.98; }
.hero-illustration .bar1 { animation: growBar 900ms cubic-bezier(.2,.9,.2,1) 200ms forwards; }
.hero-illustration .bar2 { animation: growBar 1000ms cubic-bezier(.2,.9,.2,1) 350ms forwards; }
.hero-illustration .bar3 { animation: growBar 1100ms cubic-bezier(.2,.9,.2,1) 500ms forwards; }

.hero-illustration .coins .coin { transform-origin: center; opacity: 0; }
.hero-illustration .coins .c1 { animation: riseCoin 2300ms ease-in-out 600ms infinite; }
.hero-illustration .coins .c2 { animation: riseCoin 2400ms ease-in-out 900ms infinite; }
.hero-illustration .coins .c3 { animation: riseCoin 2100ms ease-in-out 400ms infinite; }

@keyframes growBar {
  0% { transform: scaleY(0); opacity: 0.7; }
  60% { transform: scaleY(1.05); opacity: 1; }
  100% { transform: scaleY(1); opacity: 1; }
}

@keyframes riseCoin {
  0% { transform: translateY(0) scale(0.9); opacity: 0; }
  10% { opacity: 1; }
  50% { transform: translateY(-42px) scale(1.05); opacity: 1; }
  90% { opacity: 0.6; }
  100% { transform: translateY(-80px) scale(0.9); opacity: 0; }
}

